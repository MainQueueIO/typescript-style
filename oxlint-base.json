{
  "plugins": [
    "oxc",
    "typescript",
    "unicorn",
    "react",
    "import",
    "jsx-a11y",
    "jest",
    "jsdoc"
  ],
  "rules": {
    "import/no-cycle": "off",
    "react/button-has-type": 2,
    "@typescript-eslint/no-explicit-any": 2,
    "no-console": 2,
    "jest/no-identical-title": 2,
    "jest/no-alias-methods": 2,
    "jsx-a11y/label-has-associated-control": "off"
  }
}