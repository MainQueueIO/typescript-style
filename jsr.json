{
  "$schema": "https://jsr.io/schema/config-file.v1.json",
  "name": "@mainqueueio/eslint-config",
  "version": "0.1.1",
  "lint": {
    "rules": {
      "exclude": [
        "no-slow-types"
      ]
    }
  },
  "publish": {
    "include": [
      "./dist",
      "./eslint.config.mjs",
      "./prettier.config.mjs",
      "./oxlint-base.json",
      "./index.ts",
      "README.md"
    ]
  },
  "exports": {
    "./eslint": "./eslint.config.mjs",
    "./prettier": "./prettier.config.mjs",
    "./oxlint": "./oxlint-base.json",
    ".": "./index.ts"
  }
}
